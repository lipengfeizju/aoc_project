<?xml version="1.0" encoding="ISO-8859-15"?>
<launch>
  <env name="GAZEBO_MODEL_PATH" value="$(find nx_models)/models:$(optenv GAZEBO_MODEL_PATH)"/>
  <arg name="world" default="empty"/>
  
  <!-- Load world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find nx_models)/worlds/$(arg world).world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="false"/>
    <arg name="headless" value="true"/>
    <arg name="debug" value="false"/>
  </include>
  <!-- <include file="$(find nx_launch)/launch/drlpdemo/spawn_world_drlp.launch">
    <arg name="world" value="$(arg world)"/>
  </include> -->
</launch>

