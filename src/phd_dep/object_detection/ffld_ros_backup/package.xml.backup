<?xml version="1.0"?>
<package format="2">
  <name>ffld_ros</name>
  <version>0.1.0</version>
  <description>
    Fast Fourier Linear Detection - ROS Nodes
  </description>
  <author>Charles Dubout</author>
  <author>Nikolay Atanasov</author>
  <maintainer email="atanasov@seas.upenn.edu">Nikolay Atanasov</maintainer>
  <license>GPLv3</license>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->
  <build_depend>message_generation</build_depend>

  <!-- Dependencies needed to run this package. -->
  <exec_depend>message_runtime</exec_depend>

  <!-- Dependencies needed to compile and run this package. -->
  <depend>roscpp</depend>
  <depend>ffld_nx</depend>
  <depend>std_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>cv_bridge</depend>
  <depend>image_transport</depend>
</package>
